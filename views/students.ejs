<%-include('partials/header',{pageTitle:dataPassed.pageTitle})%>
    <%-include('partials/nav-bar')%>
    <table class="table">
      <tr>
        <td>No</td>
        <td>First Name</td>
        <td>Last Name</td>
        <td>Email</td>
        <td>Action</td>
      </tr>
      <%var i=0%>
      <%for(var student of dataPassed.students){%>
        <tr>
          <td><%=++i%></td>
          <td><%=student.first_name%></td>
          <td><%=student.last_name%></td>
          <td><%=student.email%></td>
          <td><a href="/students/<%=student.id%>/addsubject">add subject</a>|<a href="/students/edit/<%=student.id%>">edit</a>|<a href="/students/delete/<%=student.id%>">delete</a></td>
        </tr>
      <%}%>
    </table>
    <a href="/students/add">add student</a>
<%-include('partials/footer')%>
